                            <show_in_website>1</show_in_website>
                            <show_in_store>0</show_in_store>
                        </regex_postcode>
                        <regex_city translate="label" module="Quafzi_AddressValidation">
                            <label>Regular expression for city validation</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>200</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>0</show_in_store>
                        </regex_city>
                        <regex_telephone translate="label" module="Quafzi_AddressValidation">
                            <label>Regular expression for telephone validation</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>200</sort_order>
                            <show_in_default>1</show_in_default>
