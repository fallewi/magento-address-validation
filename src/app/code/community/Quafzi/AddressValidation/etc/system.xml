<config>
    <sections>
        <customer>
            <groups>
                <address>
                    <fields>
                        <regex_firstname translate="label" module="Quafzi_AddressValidation">
                            <label>Regular expression for firstname validation</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>200</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>0</show_in_store>
                        </regex_firstname>
                        <regex_lastname translate="label" module="Quafzi_AddressValidation">
                            <label>Regular expression for lastname validation</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>210</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>0</show_in_store>
                        </regex_lastname>
                        <regex_company translate="label" module="Quafzi_AddressValidation">
                            <label>Regular expression for company validation</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>220</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>0</show_in_store>
                        </regex_company>
                        <regex_street2 translate="label" module="Quafzi_AddressValidation">
                            <label>Regular expression for validation of street line 2</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>230</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
